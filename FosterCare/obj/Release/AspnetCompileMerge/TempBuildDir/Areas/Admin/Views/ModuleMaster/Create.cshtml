@model FosterCare.ModuleMasterTbl

@{
    ViewBag.Title = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}
<section class="content-header">
    <h1>Add Module Master <small>(Add Module Master)</small></h1>
</section>
<section class="content">

    @using (Html.BeginForm("Create", "ModuleMaster", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <div class="row">
            <div class="col-md-12">
                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div class="box box-warning">
                    <div class="box-body">
                        <div class="form-group">
                            <label>Module Name</label>
                                @Html.EditorFor(model => model.ModuleName, new { htmlAttributes = new { @class = "form-control", required = "required" } })
                                @Html.ValidationMessageFor(model => model.ModuleName, "", new { @class = "text-danger" })
                        </div>

                        <div class="form-group">
                            <label>URL</label>
                                @Html.EditorFor(model => model.URL, new { htmlAttributes = new { @class = "form-control", required = "required" } })
                                @Html.ValidationMessageFor(model => model.URL, "", new { @class = "text-danger" })
                        </div>

                    </div>
                    <div class="box-footer">
                        <input type="submit" value="Create" class="btn btn-primary" />
                    </div>
                </div>
            </div>
        </div>
    }

    <div>
        <a href="/Admin/ModuleMaster/Index" class="btn btn-dropbox">Back to List</a>
    </div>
</section>